{
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "hosting": {
    "public": "frontend/build/web",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [
      {
          "source": "*",
          "headers": [
              {
                  "key": "Access-Control-Allow-Origin",
                  "value": "*"
              }
          ]
      }
  ]
  }
}
